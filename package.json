{
  "name": "elavat-kuvat",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "publish:worker": "wrangler publish",
    "publish:assets": "b2 sync --keepDays 1 --replaceNewer .svelte-kit/output/client b2://elavat-kuvat && b2 sync --keepDays 1 --replaceNewer static b2://elavat-kuvat",
    "publish": "npm run publish:assets && npm run publish:worker",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.0.0-next.166",
    "@types/cookie": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.3.4",
    "cssnano": "^5.0.8",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.4.0",
    "simple-svelte-autocomplete": "^2.2.3",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^2.2.15",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@cloudflare/wrangler": "^1.19.3",
    "@fontsource/fira-mono": "^4.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@lukeed/uuid": "^2.0.0",
    "@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.21",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@vime/svelte": "^5.0.33",
    "cookie": "^0.4.1",
    "svelte-fa": "^2.3.3"
  }
}
